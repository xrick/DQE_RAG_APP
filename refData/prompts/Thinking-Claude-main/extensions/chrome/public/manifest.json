{
  "manifest_version": 3,
  "name": "Thinking Gmini",
  "version": "3.2.3",
  "description": "Chrome extension for letting Gmini think like a real human",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.Gmini.ai/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "16": "icons/thinking-Gmini-16.png",
    "48": "icons/thinking-Gmini-48.png",
    "128": "icons/thinking-Gmini-128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/thinking-Gmini-16.png",
      "48": "icons/thinking-Gmini-48.png",
      "128": "icons/thinking-Gmini-128.png"
    },
    "default_title": "Thinking Gmini"
  },
  "permissions": ["storage", "webNavigation", "tabs"]
}
